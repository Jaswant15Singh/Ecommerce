  <section class="py-5 overflow-hidden categories-section">
    <div class="container-lg">
      <div class="row">
        <div class="col-md-12">
          <div class="section-header d-flex flex-wrap justify-content-between mb-3">
            <h2 class="section-title">Category</h2>
            <div class="d-flex align-items-center gap-2 ">
              <div class="swiper-buttons">
                <button class="swiper-prev category-carousel-prev btn btn-yellow">❮</button>
                <button class="swiper-next category-carousel-next btn btn-yellow">❯</button>
              </div>
              <a href="/categories" class="btn btn-primary me-2 category-swiper">View All</a>        
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-md-12">
          <div class="category-carousel swiper">
            <div class="swiper-wrapper">
              <% if (category && category.length> 0) { %>
                <% category.forEach(e=> { %>
              <a  href="javascript:void(0)" class="nav-link swiper-slide text-center"onclick="fetchCategoryProducts(<%= e.id %>, this)"><img src="<%= e.category_image %>" 

                alt="<%= e.category_name %>"
                width="130px" 
                style="object-fit: cover; border-radius: 0.7rem;" 
                loading="lazy">
           
                    <h4 class="fs-6 mt-3 fw-normal category-title">
                      <%= e.category_name %>
                    </h4> 
                  </a>
                  <% }); %>
                    <% } else { %>
                      <p>No products available.</p>
                      <% } %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <section class="py-5 overflow-hidden mobile-products">
    <div class="container-lg">
      <div class="row">
        <div class="col-md-12">
          <div class="section-header d-flex flex-wrap justify-content-between mb-3">
            <h2 class="section-title">Products</h2>
            <div class="d-flex align-items-center gap-2">
              <div class="swiper-buttons">
                <button class="swiper-prev product-carousel-prev btn btn-yellow">❮</button>
                <button class="swiper-next product-carousel-next btn btn-yellow">❯</button>
              </div>
              <a href="/product" class="btn btn-primary me-2 category-swiper">View All</a>        
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-md-12">
          <div class="product-carousel swiper" style="overflow: visible;">
            <div class="swiper-wrapper">
              <% if (products_instock && products_instock.length> 0) { %>
                <% products_instock.forEach(e=> { %>
              <div class="nav-link swiper-slide text-center">
                <div style="height: 160px;width: 160px;">
                  <img src="<%= e.product_image %>" 
               
                image-product-id="<%= e.id %>" 
                image-product-name="<%= e.product_name %>"
                alt="<%= e.product_name %>" 
                  height="100%" width="100%"
                style="object-fit: cover; border-radius: 0.7rem;" 
                id="pr-Img" 
                loading="lazy">
                </div>
           
                      <div class="eye_icon" 
                      style="display: block !important; background-color: white !important; height: 30px !important; width: 30px !important;"
                      onclick="toggleProductDetailss(this, 
                        '<%= e.id %>',
                        '<%= e.product_name.replace(/'/g, '\\\'') %>', 
                        '<%= e.product_description.replace(/'/g, '\\\'') %>',
                        '<%= e.product_image_three %>',
                        '<%= e.mrp_price %>',
                        '<%= e.product_price %>',
                        '<%= (e.health_benefits ? e.health_benefits.replace(/'/g, "\\'") : '') %>',
                        '<%= e.type %>',
                        <%= JSON.stringify(e.batch_details) %>
                      )">
                        <p style="margin-top: -4.5px !important;">
                          <i class="fa-solid fa-eye text-dark" style="color: black !important; border: black !important;"></i>
                        </p>
                    </div>
                    
                    <h4 class="fs-6 mt-3 fw-normal category-title product-name" style="text-align: center;">
                      <%= e.product_name %>
                    </h4> 

                    <h4 class="fs-6 fw-normal product-desc">
                      <%= e.product_description %>
                    </h4>
                    <% if(e.type==="PIECES"){%>  
                      <div class="d-flex justify-content-center align-items-center gap-2 product-prices" style="font-size: 14px;">
                          <del><span>&#8377;</span>
                              <%= e.mrp_price %>
                          </del>
                          <span class="text-dark fw-semibold"><span>&#8377;</span>
                              <%= e.product_price %> /
                              pc

                          </span>
                      </div>
                      <%}%>
                      <% if (e.type === "UNITS") { %>
                        <% if (Array.isArray(e.batch_details) && e.batch_details.length > 0) { %>
                          <% const item = e.batch_details[0]; %>
                          <div class="d-flex justify-content-center align-items-center gap-2 product-prices"style="font-size: 14px;">
                              <div>
                                <del>
                                  <span>&#8377;</span><%= item.mrp %>
                                </del>
                                <span class="text-dark fw-semibold">
                                  <span>&#8377;</span><%= item.discount_price %>
                                  /
                                  <%= item.label_name %>
                                </span>
                              </div>
                          </div>
                        <% } %>
                      <% } %>
                      <% if (e.in_stock_status) { %>
                        <div class="button-area p-3 pt-0" style="width: 100% !important;">
                          
                          <div class="row g-1 mt-2">
                    
                            <div 
                            class=" d-none" 
                            style="align-items: center;"
                          >
                              <button 
                                type="button" 
                                class="no-styles sub" 
                               
                                onclick="changeQuantity('<%= e.id %>', -1)"
                              >
                                <span style="width: 7px;">-</span>
                              </button>
                              <input 
                                type="number" 
                                name="quantity"
                                class="form-control border-dark-subtle input-number quantity cartquantity mx-2"
                                value="1" 
                                min="1"
                                max="<%= e.in_stock %>" 
                                data-stock="<%= e.in_stock %>" 
                                data-product-id="<%= e.id %>" 
                                inputmode="numeric" 
                                oninput="this.value = this.value.replace(/[^0-9]/g, '')"
                              >
                              <button 
                                type="button" 
                                class="no-styles add" 
                                onclick="changeQuantity('<%= e.id %>', 1)"
                              >
                                <span style="width: 7px;">+</span>
                              </button>
                            </div>
                          
                            <div 
                              class="col-lg-12 col-md-12 col-sm-12" 
                              style="display: flex; align-items: center;"
                            >
                              <button 
                                data-product-id="<%= e.id %>"
                                class="btn btn-primary rounded-1 p-2 fs-7 btn-carts"
                                data-typeattribute="<%= e.type %>"
                              >
                                Add to Cart
