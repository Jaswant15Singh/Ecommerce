                        <a href="#" title="<%= e.product_name %>">
                          <img src="<%= e.product_image_two %>" image-product-id="<%= e.id %>" image-product-name="<%= e.product_name %>" alt="<%= e.product_name %>" class="tab-image image_two" loading="lazy">
