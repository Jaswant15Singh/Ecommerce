                                <button 
                                  type="button" 
                                  class="no-styles add" 
                                  onclick="changeQuantity('<%= e.id %>', 1)"
                                >
                                  <span style="width: 7px;">+</span>
                                </button>
                              </div>
                            
                              <div 
                                class="col-lg-12 col-md-12 col-sm-12" 
                                style="display: flex; align-items: center;"
                              >
                                <button 
                                  data-product-id="<%= e.id %>"
                                  class="btn btn-primary rounded-1 p-2 fs-7 btn-cart"
                                  data-typeattribute="<%= e.type %>"
                                >
                                  <svg width="18" height="18"></svg> Add to Cart
                                </button>
                              </div>
                            </div>
                            
                            
                          </div>
                        <% } else { %>
                          <div class="button-area p-3 pt-0">
                            <div class="row g-1 mt-2">
                              <div class="col-12">
                                <button class="btn btn-primary rounded-1 p-2 fs-7 btn-cart"  style="background-color: red; opacity: 0.6;">
                                  <svg width="18" height="18"></svg> Out Of Stock
