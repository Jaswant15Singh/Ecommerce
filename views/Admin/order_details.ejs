    <main>
        <div class="left">
            <h1 style="margin-top: 1rem;">Order Details</h1>
           

            <ul class="breadcrumb"></ul>
        </div>
        <div class="head-title">
            <!-- <div class="left">
                <h1 style="margin-top: 1rem;">Order Details</h1>
                <ul class="breadcrumb"></ul>
            </div> -->
            <div class="row w-100 justify-content-between ">
                <div class="col-md-6  d-flex justify-content-start">
                    <div class="sm-main text-center">
                        <select id="paymentStatusDropdown" class="form-select mb-2">
                            <option value="">Select Payment Status</option>
                            <option value="PENDING">Pending</option>
                            <option value="FAILED">Failed</option>
                            <option value="SUCCESS">Success</option>
                        </select>
            
                        <select id="deliveryStatusDropdown" class="form-select mb-3">
                            <option value="">Select Delivery Status</option>
                            <option value="PENDING">Pending</option>
                            <option value="SUCCESS">Success</option>
                        </select>
            
                        <div class="d-flex justify-content-center gap-2 mb-3">
                            <input type="date" id="startDate" class="form-control" style="max-width: 150px;" />
                            <input type="date" id="endDate" class="form-control" style="max-width: 150px;" />
                        </div>
            
                        <button id="filterButton" class="btn btn-primary">Submit</button>
                    </div>
                </div>
            
                <div class="col-md-6 d-flex justify-content-start flex-column align-items-end">
                    <button class="btn btn-primary mb-2" id="downloadExcelButton">Download Excel</button>
                    <button class="btn btn-primary" id="downloadExcelIndividualButton">Individual Order Excel</button>
                </div>
            </div>
            

        </div>

        <div class="table-data" style="overflow: auto;">
            <div class="order">
                <div class="head">
                    <h3>Orders</h3>
                </div>
                <table id="ordertable">

                    <thead>
                        <tr>
                            <th>Sr No</th>
                            <th>Order</th>
                            <th>Actions</th>
                            <th>Customer Name</th>
                            <th>Customer Email</th>
                            <th>Customer Contact</th>
                            <th class="address">Customer Address</th>
                            <th>Pin-Code</th>
                            <th>Total Price</th>
                            <th>Payment Status</th>
                            <th>Delivery Status</th>
                            <th>Ordered Date</th>
                            <th>Deleivered Date</th>
                            <th>Payment Type</th>
                            <th>Transaction Id</th>
                            <th>Payment Bnak/Channel</th>
                        </tr>
                    </thead>
                    <tbody>


                    </tbody>

                </table>
                <div id="pagination" style="margin-top: 20px;"></div>
            </div>
        </div>
    </main>
